function main () as int {
    define i as int;
    define j as int;
    prints("While Loop");
    while (i < 10) {
        print(i);
        incr i by 3;
    }
    prints("Repeat Until Loop");
    repeat {
        print(i);
        decr i by 2;
    } until i < 0;
    prints("For Loop");
    for i from 1 to 10 every 1 {
        print(i);
    }
    prints("Tilde");
    print(i);
}