function main () as int {
    define arr as string[3];
    define i as int;
    define s as string;
    arr[0] <- "Hello";
    arr[1] <- "World";
    arr[2] <- "!";
    for each s in arr {
        prints(s);
    }
    for i from len(arr) - 1 to 0 every -1 {
        prints(arr[i]);
    }
    s <- 'c' as string;
    prints(s);
}